{
  "name": "cooljs-examples",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev:react-hooks": "cross-env APP='react-hooks-variant' concurrently \"yarn mock\" \"vite\"",
    "dev:reatom": "cross-env APP='reatom-variant' concurrently \"yarn mock\" \"vite\"",
    "dev:redux-saga": "cross-env APP='redux-saga-variant' concurrently \"yarn mock\" \"vite\"",
    "dev:redux-thunk": "cross-env APP='redux-thunk-variant' concurrently \"yarn mock\" \"vite\"",
    "unit-test": "vitest",
    "components-test": "playwright test -c playwright-ct.config.ts",
    "e2e-test:react-hooks": "concurrently \"yarn dev:react-hooks\" \"playwright test -c playwright-e2e.config.ts\"",
    "e2e-test:reatom": "concurrently \"yarn dev:reatom\" \"playwright test -c playwright-e2e.config.ts\"",
    "e2e-test:redux-saga": "concurrently \"yarn dev:redux-saga\" \"playwright test -c playwright-e2e.config.ts\"",
    "e2e-test:redux-thunk": "concurrently \"yarn dev:redux-thunk\" \"playwright test -c playwright-e2e.config.ts\"",
    "dev": "concurrently \"yarn mock\" \"vite\"",
    "build": "tsc && vite build",
    "mock": "mcs",
    "lint": "eslint . --fix",
    "lint-inspector": "npx @eslint/config-inspector",
    "format": "prettier --write .",
    "stylelint": "stylelint \"**/*.css\"",
    "pretty": "yarn format && yarn lint --fix && yarn stylelint --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.1",
    "@reatom/framework": "^3.4.37",
    "@reatom/logger": "^3.6.0",
    "@reatom/npm-react": "^3.8.4",
    "@reatom/persist-web-storage": "^3.3.3",
    "@reatom/timer": "^3.6.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.62.15",
    "@tanstack/react-router": "1.15.23",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-number-format": "^5.4.3",
    "react-redux": "^9.2.0",
    "redux-saga": "^1.3.0",
    "sonner": "1.4.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/experimental-ct-react": "1.49.1",
    "@playwright/test": "1.49.1",
    "@reatom/eslint-plugin": "^3.4.3",
    "@reatom/testing": "^3.4.8",
    "@siberiacancode/eslint": "^2.6.0",
    "@siberiacancode/prettier": "^1.1.1",
    "@siberiacancode/stylelint": "^1.1.1",
    "@tanstack/eslint-plugin-query": "^5.62.15",
    "@tanstack/eslint-plugin-router": "^1.92.7",
    "@tanstack/router-devtools": "^1.40.0",
    "@tanstack/router-vite-plugin": "1.15.11",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "playwright": "1.49.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.3.0",
    "mock-config-server": "3.6.1",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "redux-saga-test-plan": "^4.0.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "6.0.7",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --no-error-on-unmatched-pattern --fix"
    ]
  }
}
